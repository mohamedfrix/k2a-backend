{
  "name": "k2a-backend",
  "version": "1.0.0",
  "description": "K2A Car Rental Backend API",
  "main": "dist/src/server.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/src/server.js",
    "dev": "nodemon src/server.ts",
    "postinstall": "prisma generate",
    "validate-env": "node scripts/validate-env.js",
    "db:status": "node scripts/check-database.js",
    "test": "ts-node -r tsconfig-paths/register tests/runTests.ts",
    "test:auth": "ts-node -r tsconfig-paths/register tests/workingAuthTests.ts",
    "test:vehicles": "ts-node -r tsconfig-paths/register tests/vehicleTests.ts",
    "test:utils": "ts-node -r tsconfig-paths/register tests/utilTests.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "npx ts-node -r tsconfig-paths/register scripts/seed-all.ts",
    "db:seed:admin": "npx ts-node -r tsconfig-paths/register scripts/seed-admin.ts",
    "db:seed:vehicles": "npx ts-node -r tsconfig-paths/register scripts/seed-vehicles.ts",
    "db:reset": "npx prisma migrate reset --force && npm run db:seed"
  },
  "keywords": [
    "nodejs",
    "express",
    "typescript",
    "prisma",
    "auth",
    "jwt"
  ],
  "author": "K2A Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.0.0",
    "@types/multer": "^2.0.0",
    "@types/node-fetch": "^2.6.13",
    "@types/sharp": "^0.31.1",
    "@types/uuid": "^10.0.0",
    "@types/xlsx": "^0.0.36",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "minio": "^8.0.5",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.10.1",
    "prisma": "^6.0.0",
    "sharp": "^0.34.3",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/form-data": "^2.2.1",
    "@types/jest": "^29.5.14",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.9.0",
    "@types/nodemailer": "^6.4.19",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "eslint": "^9.15.0",
    "form-data": "^4.0.4",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
